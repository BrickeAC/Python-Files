# Question 1) a)
a=int(2)
b=int(2)
c=int(9)

if a+b>=c and b+c>=a and c+a>=b:
  print ("Yes, these three lengths can form a triangle")
else:
   print ("No, these three lengths cannot form a triangle")



# Question 1) b)
size = int(5)

for i in range(size):
    for j in range(size):
        print('*' if i in [0, size-1] or j in [0, size-1] else ' ', end='')
    print()